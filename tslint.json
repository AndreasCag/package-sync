{
  "extends": "tslint-microsoft-contrib",
  "rules": {
    "typedef": [false],
    "no-relative-imports": [false],
    "no-suspicious-comment": [false],
    "no-this-assignment": [true, {"allow-destructuring": true}],
    "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
    "no-console": [false]
  }
}
