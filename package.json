{
  "name": "package-sync",
  "version": "1.1.0",
  "description": "Helps to keep packages in sync inside your team",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "compile": "tsc",
    "upd": "node ./dist/cli/index.js --update",
    "check": "node ./dist/cli/index.js --check",
    "soft-check": "node ./dist/cli/index.js --soft-check",
    "inst": "node ./dist/cli/index.js --install"
  },
  "bin": {
    "package-sync": "./dist/cli/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndreasCag/package-sync.git"
  },
  "keywords": [
    "package",
    "sync",
    "package-sync",
    "synchronise"
  ],
  "author": "AndreasCag",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AndreasCag/package-sync/issues"
  },
  "homepage": "https://github.com/AndreasCag/package-sync#readme",
  "dependencies": {
    "@types/minimist": "^1.2.0",
    "minimist": "^1.2.0",
    "lodash": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^11.11.4",
    "tslint": "^5.14.0",
    "tslint-microsoft-contrib": "^6.1.0",
    "typescript": "^3.3.4000"
  }
}
